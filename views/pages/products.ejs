<% include ../partials/header %>

<table class="table">

    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Price</th>
        </tr>
    </thead>

    <tbody>
    <% products.forEach(function(product){ %>
      <tr>
            <td scope="row"><%= product.id %></td>
            <td><%= product.title %></td>
            <td>$<%= product.price %></td>
            <td><a class="btn btn-primary" href="/products/<%=product.id%>" role="button">Edit</a>
            <a name="" id="" class="btn btn-danger" href="#" role="button">Delete</a>
            </td>
           
        </tr>
    <% }) %>
    </tbody>

</table>


<% include ../partials/footer %>